<h1>Artist Index</h1>

<% @artists.each do |artist| %>
<div class="row">
  <div class="col s12 l3">
    <div class="card">
      <div class="card-image">
        <img src="<%=artist.image_path%>">
      </div>
      <div class="card-action">
        <a <%= link_to artist.name, artist_path(artist) %></a>
      </div>
    </div>
  </div>
  <% end %>
</div>
<%= link_to "New artist", new_artist_path %>
